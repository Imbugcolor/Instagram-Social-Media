    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-auth-library": "^8.8.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.10.1",
    "node-fetch": "^2.6.9",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12",
    "peer": "^1.0.0",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
